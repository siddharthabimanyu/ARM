#include"lcd header.h"
unsigned char CGRAM_LUT[]={0x17,0x14,0x14,0x1f,0x05,0x05,0x1d,0x00,0x0e,0x1b,0x11,0x11,0x11,0x11,0x1f,0x00};
void CGRAM_WRITE(unsigned char);
int main()
{
LCD_INIT();
CGRAM_WRITE(16);
LCD_CMD(0X80);		  
LCD_DATA(0);
}
void CGRAM_WRITE(unsigned char n )
{
unsigned char i;
LCD_CMD(0x40);
for(i=0;i<n ;i++)
{
LCD_DATA(CGRAM_LUT[i]);
} 
}
